{
	"name": "plagiarism-report",
	"version": "1.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve demo-app",
		"build": "ng build demo-app",
		"lint": "ng lint",
		"lib:pack": "npm pack ./dist/plagiarism-report",
		"local:package": "npm run lib:prettier && npm run lib:build && npm run lib:pack",
		"lib:test": "ng test plagiarism-report",
		"lib:lint": "ng lint plagiarism-report",
		"lib:prettier": "prettier --write projects/plagiarism-report/**/*.{ts,js,css,scss,html} projects/demo-app/**/*.{ts,js,css,scss,html}",
		"lib:version": "cd projects/plagiarism-report && npm version patch",
		"lib:build": "ng build plagiarism-report --configuration production",
		"lib:copy-readme": "copy ../README.md dist/plagiarism-report",
		"lib:publish": "cd dist/plagiarism-report && npm publish",
		"script:extract-version": "node scripts/extract-version.js",
		"package": "npm run lib:prettier && npm run lib:version && npm run script:extract-version && npm run lib:build && npm run lib:publish"
	},
